{
  "name": "hot-utils",
  "version": "1.2.6",
  "description": "various NodeJS utils with TS support",
  "repository": {
    "url": "https://github.com/petarzarkov/hotstuff"
  },
  "keywords": [
    "node",
    "ts",
    "hot",
    "hot-utils",
    "utils",
    "url",
    "object",
    "http"
  ],
  "author": "Petar Zarkov",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "clean": "tsc --build --clean",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.json",
    "build:watch": "npm run build -- --watch",
    "build:speed": "tsc -p tsconfig.speed.json",
    "start": "node lib/index.js",
    "lint": "tsc --noEmit && eslint -c .eslintrc.js --ext .ts,.tsx ./src",
    "lint:fix": "npm run lint --fix",
    "version:update": "node ./scripts/updateVersion.js",
    "gen:log": "node ./scripts/genLog.js",
    "test": "set NODE_ENV=test&& jest --config=./config/jest.utest.json --forceExit --detectOpenHandles",
    "test:coverage": "set NODE_ENV=test&& npm run test -- --coverage"
  },
  "dependencies": {
    "node-fetch": "3.0.0-beta.9"
  },
  "devDependencies": {
    "@types/config": "0.0.40",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "config": "3.3.6",
    "eslint": "8.9.0",
    "jest": "27.5.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  },
  "publishConfig": {
    "access": "public",
    "cache": "cache/.npm"
  },
  "files": [
    "lib",
    "index.d.ts",
    "index.js",
    "index.js.map"
  ]
}
